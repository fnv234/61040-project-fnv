{
  "name": "experience-log-demo",
  "version": "1.0.0",
  "description": "A demo application showing how to use Google's Gemini API to generate matcha tasting experience logs and AI-powered summaries",
  "main": "dist/experienceLog-tests.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/experienceLog-tests.js",
    "dev": "ts-node --transpile-only experienceLog-tests.ts",
    "test": "npm run build && node dist/experienceLog-tests.js",
    "crud": "npm run build && node -e \"require('./dist/experienceLog-tests.js').testBasicCRUD()\"",
    "averages": "npm run build && node -e \"require('./dist/experienceLog-tests.js').testAverages()\"",
    "summary": "npm run build && node -e \"require('./dist/experienceLog-tests.js').testAIProfileSummary()\""
  },
  "keywords": ["gemini", "ai", "matcha", "experience-log", "typescript"],
  "author": "Student Demo",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0"
  }
}
